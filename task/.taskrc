# Files
data.location=~/.task
color=on

color.uda.priority.H=color255
color.uda.priority.L=color245
color.uda.priority.M=color250

uda.priority.values=H,M,,L,P

urgency.uda.priority.H.coefficient=6.0
urgency.uda.priority.M.coefficient=3.9
urgency.uda.priority..coefficient=1.8
urgency.uda.priority.L.coefficient=0
urgency.uda.priority.P.coefficient=-5

default.project=Inbox
list.all.projects=yes
alias.@=context
context.Arch=project:Arch
context.Study=project:Study
context.Home=project:Home
context.Server=project:Server
context.Books=project:Books
context.Personal=projects:Personal
context.Inbox=projects:Inbox
context.Tui=projects:Tui


# My Modify Color theme
include ~/.mytask.theme
context=Tui
